    ".": {
      "import": "./lib/wrapper.mjs",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./api": {
      "import": "./lib/api/wrapper.mjs",
      "require": "./lib/api/index.js",
      "types": "./lib/api/index.d.ts"
    },
    "./lib/*": {
      "require": "./lib/*.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./lib/index.d.ts",
