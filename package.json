  , "connect" : "1.8.1"
  , "nopt" : "1.0.10"
  , "rimraf" : "1.0.8"
  , "mkdirp" : "0.2.1"
  , "cucumber-html": "0.2.0"
  , "findit": "0.1.1"
  , "coffee-script": "1.1.2"
  , "ansi-color": "0.2.1"
  , "amdefine": ">=0.0.2"
  }
, "scripts" :
  { "test" : "./bin/cucumber.js && jasmine-node spec" }
, "bin": { "cucumber.js": "./bin/cucumber.js", "cucumber-js": "./bin/cucumber.js" }
, "licenses" :
  [ { "type" : "MIT"
    , "url" : "http://github.com/cucumber/cucumber.js/LICENSE"
    }
  ]
}
